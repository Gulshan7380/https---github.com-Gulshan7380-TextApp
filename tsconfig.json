{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions" : {
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@utils/*": ["src/utils/*"],
      "@screens/*": ["src/screens/*"],
    }
  }
}